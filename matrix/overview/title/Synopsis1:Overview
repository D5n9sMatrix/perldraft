sub f(x, k) is export {
    sub a(@p) is export {
        say @p;
    }
    sub typedef($p) is export {
        say $p;
    }
    my $x = typedef -- "run compile" if -0[append Cool] - Array["/default/library/"] - a [append Cool];
    my $k = $x.imp;

    say $k;
}

